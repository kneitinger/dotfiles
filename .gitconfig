[user]
    email = kyle@kneit.in
    name = Kyle Kneitinger
[color]
    ui = true
[core]
    excludesfile = ~/.git_excludes
[github]
    user = kneitinger
[alias]
    a  = add
    c  = commit
    ch  = checkout
    chb  = checkout -b
    c- = checkout --
    d  = diff
    puo = push -u origin
    rh = reset HEAD
    s  = status
    st = stash
    sta = stash apply
[rerere]
    enabled = true
    autoupdate = true
